@model WebApplication.Models.UserRoles

@{
    ViewBag.Title = "AddRoleToUser";
}

<h2>AddRoleToUser</h2>

@using (Html.BeginForm("AddRoleToUser", "Account")) { 
    <div>
        @Html.LabelFor(m => m.email)
        @Html.TextBoxFor(m => m.email)
        
    </div>

    <div>
        @Html.DropDownListFor(m => m.selectedRole, new SelectList(Model.Roles))
    </div>

    <button type="submit">Add</button>
}
